(defun exercise-submit ()
  "Submits currently active file to exercism.io"
  (interactive)
  (save-buffer)
  (shell-command (format "exercism submit %s" buffer-file-name)))

(defun leap ()
  "Moves the cursor forward until it finds a whitespace"
  (interactive)
  (re-search-forward "[\s\t\r\n\f]+" nil 1))
(global-set-key (kbd "M-N") 'leap)
